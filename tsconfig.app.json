{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["src"],
  "exclude": [],
  "compilerOptions": {
    "baseUrl": ".",
    "composite": true,
    "paths": {
      "@/*": ["./src/*"]
    },

    /* Linting */
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true
  },
  "strict": true
}
